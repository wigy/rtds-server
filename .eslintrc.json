{
  "extends": [
    "semistandard"
  ],
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "space-before-function-paren": "off",
    "padded-blocks": "off",
    "eqeqeq": "error",
    "object-curly-spacing": "off"
  }
}
